APP_KEY=
APP_URL=https://bookstack.chrisx.xyz
APP_TIMEZONE=America/New_York

DB_HOST=localhost
DB_PORT=3306
DB_DATABASE=bookstack
DB_USERNAME=bookstack
DB_PASSWORD={{ mysql_bookstack_password.stdout }}

SESSION_LIFETIME=120
SESSION_SECURE_COOKIE=true

AUTH_METHOD=saml2
SAML2_NAME='TinyNET Account'
SAML2_EMAIL_ATTRIBUTE=email
SAML2_EXTERNAL_ID_ATTRIBUTE=uid
SAML2_DISPLAY_NAME_ATTRIBUTES=displayName
SAML2_IDP_ENTITYID=https://tinynet-iam.chrisx.xyz/auth/realms/tinynet/protocol/saml/descriptor
SAML2_AUTOLOAD_METADATA=true
SAML2_USER_TO_GROUPS=true
SAML2_GROUP_ATTRIBUTE=member
SAML2_REMOVE_FROM_GROUPS=true

DISABLE_EXTERNAL_SERVICES=true
