APP_ENV=production
APP_DEBUG=false
APP_KEY=

DEFAULT_LANGUAGE=en_US
DEFAULT_LOCALE=equal
TZ=America/New_York
TRUSTED_PROXIES=10.69.233.10

LOG_CHANNEL=stack
APP_LOG_LEVEL=notice
AUDIT_LOG_LEVEL=info

DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=firefly
DB_USERNAME=firefly
DB_PASSWORD={{ mysql_firefly_password.stdout }}

CACHE_DRIVER=file
SESSION_DRIVER=file

COOKIE_PATH="/firefly-iii"
COOKIE_SECURE=true
COOKIE_SAMESITE=lax

MAIL_MAILER=log
MAIL_HOST=null
MAIL_PORT=25
MAIL_FROM=
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_ENCRYPTION=null

SEND_REGISTRATION_MAIL=true
SEND_ERROR_MESSAGE=true
SEND_LOGIN_NEW_IP_WARNING=false
SEND_REPORT_JOURNALS=false

ENABLE_EXTERNAL_MAP=false
MAP_DEFAULT_LAT=0.0
MAP_DEFAULT_LONG=0.0
MAP_DEFAULT_ZOOM=6

LOGIN_PROVIDER=eloquent
AUTHENTICATION_GUARD=remote_user_guard
AUTHENTICATION_GUARD_HEADER=REMOTE_USER
AUTHENTICATION_GUARD_EMAIL=
CUSTOM_LOGOUT_URI="https://tinynet.chrisx.xyz/auth/openid-connect?logout=https://tinynet.chrisx.xyz"

DISABLE_FRAME_HEADER=false
DISABLE_CSP_HEADER=false
SEND_TELEMETRY=false
ALLOW_WEBHOOKS=false

# Leave the following configuration vars as is.
# Unless you like to tinker and know what you're doing.
APP_NAME=FireflyIII
ADLDAP_CONNECTION=default
BROADCAST_DRIVER=log
QUEUE_DRIVER=sync
CACHE_PREFIX=firefly
PUSHER_KEY=
IPINFO_TOKEN=
PUSHER_SECRET=
PUSHER_ID=
DEMO_USERNAME=
DEMO_PASSWORD=
IS_HEROKU=false
FIREFLY_III_LAYOUT=v1
APP_URL=http://localhost
